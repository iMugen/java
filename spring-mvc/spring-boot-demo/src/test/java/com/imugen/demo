food_business_license
small_business_store_record_certificate
circulation_license
none

CREATE TABLE `on_site_verify` (
  `id` int(11) NOT NULL COMMENT '主键',
  `verify_num` int(11) DEFAULT NULL COMMENT '现场核查数',
  `business_rectification_num` int(11) DEFAULT NULL COMMENT '整改通知书数',
  `normal_operating_num` int(11) DEFAULT NULL COMMENT '正常营业数',
  `network_business_num` int(11) DEFAULT NULL COMMENT '网络经营数',
  `verify_qualified_num` int(11) DEFAULT NULL COMMENT '核查合格数',
  `verify_disqualification_num` int(11) DEFAULT NULL COMMENT '核查不合格数',
  `normal_operation_num` int(11) DEFAULT NULL COMMENT '正常经营',
  `suspension_operation_num` int(11) DEFAULT NULL COMMENT '暂停经营',
  `stop_operation_num` int(11) DEFAULT NULL COMMENT '停止经营',
  `with_business_license_num` int(11) DEFAULT NULL COMMENT '有营业执照',
  `without_business_license_num` int(11) DEFAULT NULL COMMENT '无营业执照',
  `food_business_license_num` int(11) DEFAULT NULL COMMENT '食品经营许可证',
  `small_business_store_record_certificate_num` int(11) DEFAULT NULL COMMENT '小经营店备案证',
  `circulation_license_num` int(11) DEFAULT NULL COMMENT '流通许可证',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


1：现场核查数；2：整改通知书数；3：正常营业；4：网络经营